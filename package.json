{
  "name": "react-calendar-app",
  "version": "1.0.0",
  "description": "A blank express app preconfigured with karma configuration and babel transpilation",
  "main": "src/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "lint": "eslint src",
    "start": "nodemon src/index.js --exec babel-node",
    "start-debug": "DEBUG=express:* nodemon src/index.js --exec babel-node",
    "serve": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thoiberg/blank-express-app.git"
  },
  "author": "Tim Hoiberg",
